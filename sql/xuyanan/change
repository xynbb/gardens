  `price` decimal(10,2) unsigned NOT NULL DEFAULT '0.00' COMMENT '价格',
  `number` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '数量',
  `is_free` tinyint(1) unsigned NOT NULL DEFAULT '0' COMMENT '是否免费0否，1是',
  `user_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '使用时间',
  `buy_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '购买时间',
  `create_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='订单商品表';


