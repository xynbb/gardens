CREATE TABLE `garden` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '编号',
  `name` varchar(50) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '名称',
  `sort` smallint(5) NOT NULL DEFAULT '0' COMMENT '排序',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '状态(-1删除,0禁用,1正常)',
  `create_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=9 DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='园表';

CREATE TABLE `area` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '编号',
  `garden_id` int(11) NOT NULL DEFAULT '0' COMMENT '园id',
  `name` varchar(50) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '名称',
  `sort` smallint(5) NOT NULL DEFAULT '0' COMMENT '排序',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '状态(-1删除,0禁用,1正常)',
  `create_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='区表';

CREATE TABLE `rows` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '编号',
  `garden_id` int(11) NOT NULL DEFAULT '0' COMMENT '园id',
  `area_id` int(11) NOT NULL DEFAULT '0' COMMENT '区id',
  `name` varchar(50) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '名称',
  `sort` smallint(5) NOT NULL DEFAULT '0' COMMENT '排序',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '状态(-1删除,0禁用,1正常)',
  `create_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='排表';


CREATE TABLE `vitrotype` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT '编号',
  `name` varchar(50) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '瓷像别名',
  `norms_id` smallint(5) unsigned NOT NULL DEFAULT '0' COMMENT '规格id',
  `shape_id` smallint(5) unsigned NOT NULL DEFAULT '0' COMMENT '形状id',
  `color_id` smallint(5) unsigned NOT NULL DEFAULT '0' COMMENT '颜色id',
  `type_id` smallint(5) unsigned NOT NULL DEFAULT '0' COMMENT '类型id',
  `price` decimal(10,2) unsigned NOT NULL DEFAULT '0.00' COMMENT '价格',
  `note` text CHARACTER SET utf8 COMMENT '描述',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '状态(-1删除,0禁用,1正常)',
  `create_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='瓷像';


CREATE TABLE `services` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT '编号',
  `name` varchar(50) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '瓷像别名',
  `type_id` smallint(5) unsigned NOT NULL DEFAULT '0' COMMENT '服务类型id',
  `price` decimal(10,2) unsigned NOT NULL DEFAULT '0.00' COMMENT '价格',
  `note` text CHARACTER SET utf8 COMMENT '描述',
  `is_change_price` tinyint(1) unsigned NOT NULL DEFAULT '0' COMMENT '是否可调价，0否，1是',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '状态(-1删除,0禁用,1正常)',
  `create_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='服务';



CREATE TABLE `grave` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT '编号',
  `garden_id`int(11) unsigned NOT NULL DEFAULT '0' COMMENT '园id',
  `area_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '区id',
  `rows_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '排id',
  `grave_type_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '墓型id',
  `cave_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '墓穴id',
  `burial_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '安葬方式id',
  `name` varchar(100) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '名称',
  `now_type` tinyint(1) unsigned NOT NULL DEFAULT '1' COMMENT '先墓位状态1墓位，2树木',
  `is_charge` tinyint(1) unsigned NOT NULL DEFAULT '1' COMMENT '是否收费，1是，2否',
  `price` decimal(10,2) unsigned NOT NULL DEFAULT '0.00' COMMENT '价格',
  `note` text CHARACTER SET utf8 COMMENT '描述',
  `stage` tinyint(1) NOT NULL DEFAULT '1' COMMENT '墓位状态(1在售，2已预留，3已预订，4已销售，5已迁出,6已落葬，7禁售)',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '数据状态(-1删除,0禁用,1正常)',
  `create_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='墓位';


CREATE TABLE `grave_type` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT '编号',
  `name` varchar(100) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '名称',
  `shicai_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '石材属性id',
