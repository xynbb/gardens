  `note` text CHARACTER SET utf8 COMMENT '描述',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '数据状态(-1删除,0禁用,1正常)',
  `create_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='墓权证表';


CREATE TABLE `config` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT '编号',
  `name` varchar(50) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '名称',
  `code` varchar(20) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '识别码',
  `value` varchar(50) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '值',
  `parent_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '上级id',
  `price` decimal(10,2) unsigned NOT NULL DEFAULT '0.00' COMMENT '价格',
  `note` text CHARACTER SET utf8 COMMENT '描述',
  `sort` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '排序',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '状态(-1删除,0禁用,1正常)',
  `create_time` datetime NOT NULL COMMENT '创建时间',
  `update_time` datetime NOT NULL COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='配置表';


CREATE TABLE `trading` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT '编号',
  `grave_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '墓位id',
  `grave_name` varchar(100) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '墓位名称',
  `customer_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '客户id',
  `customer_name` varchar(100) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '客户名称',
  `sell_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '销售时间',
  `code` varchar(50) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '合同编码',
  `amount` decimal(10,2) unsigned NOT NULL DEFAULT '0.00' COMMENT '总价',
  `seller_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '销售id',
  `seller_name` varchar(50) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '销售名称',
  `create_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '创建id',
  `create_name` varchar(50) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT '销售名称',
  `note` text CHARACTER SET utf8 COMMENT '描述',
  `is_recommend` tinyint(1) unsigned NOT NULL DEFAULT '0' COMMENT '收费打折0否，1是',
  `recommend_type` tinyint(1) unsigned NOT NULL DEFAULT '0' COMMENT '折扣类型,0否,1折扣,2现金',
  `order_type` tinyint(2) NOT NULL DEFAULT '0' COMMENT '订单类型1墓位，2服务，3瓷像，4碑文，5商品，6维修，7迁出，8管理费，9墓权证',
  `sell_grave_type` tinyint(2) NOT NULL DEFAULT '0' COMMENT '墓位销售类型1预留，2预定，3销售',
  `stage` tinyint(2) unsigned NOT NULL DEFAULT '0' COMMENT '订单状态1创建成功,2待打折,3待收款,4已收款',
  `status` tinyint(2) NOT NULL DEFAULT '1' COMMENT '数据状态(-1删除,1正常)',
  `create_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='订单表';


CREATE TABLE `trading_price` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT '编号',
  `trading_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '订单id',
  `price` decimal(10,2) NOT NULL DEFAULT '0.00' COMMENT '价格',
  `type` tinyint(1) unsigned NOT NULL DEFAULT '0' COMMENT '价格类型1墓位价,2管理费,3墓证费,4折扣费,5续费,6维修费,7迁出费,8安葬费，9合墓费，10定金',
  `create_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '创建时间',
  `update_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='订单价格详细表';


CREATE TABLE `trading_product` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT '编号',
  `trading_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '订单id',
  `product_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '商品id',
